{
  "name": "@baento/tsflow",
  "version": "1.1.0-alpha.3",
  "author": "Antoine Balieu",
  "private": true,
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "homepage": "https://github.com/baento/tsflow#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baento/tsflow.git"
  },
  "bugs": {
    "url": "https://github.com/baento/tsflow/issues"
  },
  "scripts": {
    "build": "npm run build:swc && npm run build:tsc",
    "build:swc": "swc src -d dist --strip-leading-paths",
    "build:tsc": "tsc --project tsconfig.build.json",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "prepare": "husky",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write ."
  },
  "dependencies": {
    "@cucumber/cucumber-expressions": "^17.1.0",
    "@cucumber/gherkin": "^28.0.0",
    "@cucumber/messages": "^24.1.0",
    "callsites": "^3.0.0",
    "fast-glob": "^3.3.2",
    "lodash.clonedeep": "^4.5.0",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.0",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.5.24",
    "@swc/jest": "^0.2.36",
    "@tsconfig/recommended": "^1.0.5",
    "@types/jest": "^29.5.12",
    "@types/lodash.clonedeep": "^4.5.9",
    "@types/node": "^20.12.2",
    "husky": "^9.0.11",
    "semantic-release": "^24.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  },
  "peerDependencies": {
    "jest": "^29.7.0"
  },
  "volta": {
    "node": "22.2.0",
    "npm": "10.7.0"
  }
}
